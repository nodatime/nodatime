syntax:glob

# Editor backups and temporary files
*~
*.bak
*.swp

# IDE-generated files
*.suo
*.user
*.sln.cache

# IDE add-ins
*.dotCover
*.ncrunchproject
*.ncrunchsolution
_NCrunch*    # cache directories
.resharper   # solution
*.ReSharper  # project
_ReSharper*  # directories
StyleCop.Cache

# Auto-created thumbnails
Thumbs.db

# javac output
*.class

# Joda Time jar files
# (It's reasonably easy to end up with one in a working directory...)
joda-time*.jar

# Noda Time release (private) key
NodaTime Release.snk

# Need to use regexp syntax for rooted patterns and directory matches.
syntax:regexp

# Build output directories (anywhere)
/bin/
/obj/

# NUnit output
^TestResult\.xml$

# Sandcastle output that we're not interested in
^docs/api/fti/
^docs/api/Web\.Config$
^docs/api/[^.]+\.(aspx|xml)$
^docs/api/LastBuild.log$

# Sandcastle public API output (consumed by buildnuget.bat)
^docs/PublicApi/

# Sandcastle per-user settings
^NodaTime.shfbproj_.+$

# NuGet temporary build area (see buildnuget.bat)
^nuget/

# Generated web site
^www/_site/

# Temporary build output directories
^tmpdocs
^tmpwebdir
^tzdbtmp
^SandcastleStyleTmp

# NuGet packages (but not the configuration)
^src/packages/.*/